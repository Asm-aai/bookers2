<%= render 'info', user: @user %>
<%= render 'books/list', book: @book %>

<h1>Users</h1>

<%= @users.each do |user| %>
    <%= image_tag user.get_profile_image(100, 100) %>
    <p><%= user.name %></p>
    <p><%= user.introduction %></p>
    <p><%= link_to "Show", user_path(user.id) %></p>
<% end %>